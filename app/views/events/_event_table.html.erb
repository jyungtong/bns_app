<div id="notice" style="display: none;"></div>

<table class="bordered" id="event_table">
	<thead>
		<tr>
			<th scope="col" abbr="Number">No.</th>
			<th scope="col" abbr="Date">Event Date</th>
			<th scope="col" abbr="StartT">Start Time</th>
			<th scope="col" abbr="EndT">End Time</th>
			<th scope="col" abbr="School">School Name</th>
			<th scope="col" abbr="AIC">Agency In Charge</th>
			<th scope="col" abbr="StartP">Start Place</th>
			<th scope="col" abbr="EndP">End Place</th>
			<th scope="col" abbr="Seats">Seats Avail. / Total</th>
			<th colspan="2"></th>
		</tr>
	</thead>
	<tbody>
		<%= render @events %>
	</tbody>
</table>

<% content_for :head do %>
	<%= stylesheet_link_tag "event" %>
<% end %>
